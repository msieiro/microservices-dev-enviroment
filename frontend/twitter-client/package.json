{
  "name": "twitter-client",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "format": "prettier . --write",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "preview": "vite preview"
  },
  "dependencies": {
    "axios": "0.25.0",
    "axios-extensions": "3.1.3",
    "vue": "3.2.25",
    "vue-router": "4.0.12"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "2.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-html": "6.2.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-vue": "8.4.0",
    "eslint": "8.7.0",
    "lint-staged": "13.2.1",
    "node-sass": "7.0.1",
    "prettier": "2.5.1",
    "sass-loader": "12.4.0",
    "sass": "1.49.0",
    "vite": "2.7.13"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "frontend/*.{js,ts,vue,jsx}": [
      "npm run format",
      "npm run lint",
      "git add"
    ]
  }
}
